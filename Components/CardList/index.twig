<section class="ecocart-card-list">
    <div class="container">
        <div class="ecocart-card-list__dot-desktop">
            <img
                class="lazyload"
                data-src="{{ dot_desktop }}"
                data-sizes="auto"
                alt="dot desktop"
            />
        </div>
        <div class="ecocart-card-list__dot-mobile">
            <img
                class="lazyload"
                data-src="{{ dot_mobile }}"
                data-sizes="auto"
                alt="dot mobile"
            />
        </div>
        <div class="dot-title">
            <h3>{{ pre_title }}</h3>
        </div>
        <div class="ecocart-card-list__grid">
            {% for card in card_list %}
                <div class="ecocart-card-list__card">
                    <h3>{{ card.title }}</h3>
                    <p>{{ card.content }}</p>
                </div>
            {% endfor %}
        </div>
    </div>
</section>