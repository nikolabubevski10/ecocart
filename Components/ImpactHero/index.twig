<section class="ecocart-impact-hero">
    <div class="container">
        <div class="ecocart-impact-hero__content">
            <h1>{{ title }}</h1>
            <p>{{ content }}</p>
            <div class="ecocart-impact-hero__link">
                <a href="{{ link.url }}" class="btn btn--blue--dark">{{ link.title }}</a>
            </div>
        </div>
    </div>
    <div class="ecocart-impact-hero__marquee-container ecocart-impact-hero__marquee-container--top">
        <div class="ecocart-impact-hero__marquee" style="--play:running;--direction:normal;--duration:161.6s;--delay:0s;--iteration-count:infinite;">
            {% include 'Partials/project-row.twig' ignore missing with {project_list: project_list} only %}
        </div>
        <div class="ecocart-impact-hero__marquee" style="--play:running;--direction:normal;--duration:161.6s;--delay:0s;--iteration-count:infinite;">
            {% include 'Partials/project-row.twig' ignore missing with {project_list: project_list} only %}
        </div>
    </div>
    <div class="ecocart-impact-hero__marquee-container ecocart-impact-hero__marquee-container--bottom">
        <div class="ecocart-impact-hero__marquee" style="--play:running; --direction:reverse; --duration:161.6s; --delay:0s; --iteration-count:infinite;">
            {% include 'Partials/project-row.twig' ignore missing with {project_list: project_list} only %}
        </div>
        <div class="ecocart-impact-hero__marquee" style="--play:running; --direction:reverse; --duration:161.6s; --delay:0s; --iteration-count:infinite;">
            {% include 'Partials/project-row.twig' ignore missing with {project_list: project_list} only %}
        </div>
    </div>
</section>